<%- include("navbar.ejs")%>
    <div class="container-fluid p-5 bg-white ">
        <div class="row">
            <div class="col-md-12  text-center">
                <h3>Admin Profile</h3>
            </div>
            <div class="col-md-12">
                <table class="table table-bordered table-sm">
                    <tr>
                       <th>SN</th>
                        <th>Admin Name</th>
                        <th>Email</th>
                        <th>Password</th>

                    </tr>
                    <% for(var i=0;i<admin.length;i++)
                    {
                        %>
                        <tr>
                            <td><%=i+1%></td>
                            <td><%=admin[i].admin_name%></td>
                            <td><%=admin[i].admin_email%></td>
                            <td><%=admin[i].admin_password%></td>
                    
                            
                            
                        </tr>
                   <% }%>
                </table>
            </div>
        </div>
    </div>

    <%- include("footer.ejs")%>